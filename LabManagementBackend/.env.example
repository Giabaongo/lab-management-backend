# ====================================
# ENVIRONMENT VARIABLES TEMPLATE
# ====================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env to Git!

# Database Configuration
DB_SERVER=your-sql-server.database.windows.net
DB_NAME=LabManagementDB_v2
DB_USER=your-username
DB_PASSWORD=your-password

# JWT Configuration
JWT_KEY=your-super-secret-jwt-key-min-64-characters-for-HS512-algorithm
JWT_ISSUER=LabManagementAPI
JWT_AUDIENCE=LabManagementUsers

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
FRONTEND_URL=http://localhost:3000
FRONTEND_CALLBACK_URL=http://localhost:3000/auth/callback

# Application Settings
ASPNETCORE_ENVIRONMENT=Production
